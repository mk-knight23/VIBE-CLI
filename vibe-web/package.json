{
  "name": "vibe-web",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "description": "VIBE Web v2.0 - Documentation Hub for Multi-Agent AI Development Platform with comprehensive guides, API docs, and migration resources",
  "vibe-ecosystem": {
    "compatible_cli": "9.x",
    "compatible_extension": "5.x",
    "compatible_web": "2.x",
    "documentation_coverage": [
      "Multi-agent orchestration setup",
      "Extended thinking configuration", 
      "Web search integration guide",
      "Semantic memory usage patterns",
      "Team collaboration workflows",
      "Security scanning best practices",
      "Custom instructions management",
      "Migration from v8 to v9"
    ],
    "new_content": {
      "2.0.0": [
        "v9.0 feature documentation",
        "Interactive API explorer",
        "Migration guides and tools",
        "Team setup tutorials",
        "Security best practices",
        "Performance optimization guides"
      ]
    }
  },
  "author": "mk-knight23 <noreply@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mk-knight23/vibe",
    "directory": "vibe-web"
  },
  "bugs": {
    "url": "https://github.com/mk-knight23/vibe/issues"
  },
  "homepage": "https://github.com/mk-knight23/vibe#readme",
  "keywords": [
    "vibe",
    "vite",
    "react",
    "marketing",
    "docs",
    "ai-assistant"
  ],
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"No lint configured\"",
    "smoke": "vite build && test -d dist && echo 'web build ok'"
